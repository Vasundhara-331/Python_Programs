l = int(input("Enter the limit:"))
a = 0 
b = 1
if l < 0:
    print("Invalid input. Please enter a positive number.")
elif l == 0:
    print("Cannot generate series.")
elif l == 1:
    print(a)
else:
    print(a,end = " ")
    print(b,end = " ")
    for i in range(l-2):
        c = a + b
        print(c,end = " ")
        a = b
        b = c
